<html>
	<head>
		<% include ../_partials/head %>
        <style>
        .page-content {
            padding-top: 3.125rem;
            min-height: 100%;
            height: 100%;
        }
        </style>
    </head>
    <body class="bg-steel">
        <% include ../_partials/adminHeader%>
        <div class="page-content">
            <div class="flex-grid no-responsive-future" style="height: 100%;">
                <div class="row" style="height: 100%">
                    <div class="cell auto-size padding20 bg-white" id="cell-content">
                        <table class="dataTable border bordered" id="varshik-mulya-rate" data-role="datatable" data-auto-width="false">
                            <thead>
                            <tr>
                                <td style="width: 100px">ID (For technical purposes)</td>
                                <td class="sortable-column bg-yellow">जमिनीचा प्रकार</td>
                                <td class="sortable-column bg-yellow" style="width: 300px">वार्षिक मूल्य दर (चौ/मीटर)</td>
                            </tr>
                            </thead>
                            <tbody>
                                <% citizens.forEach((citizen) => { %>
                                    <tr>
                                        <td><%= citizen._id %></td>
                                        <td><%= citizen.name %></td>
                                        <td><%= citizen.address %></td>
                                        <td><%= citizen.ward_name %></td>
                                    </tr>
                                <% }) %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <% include ../_partials/footer%>
    </body>
</html>